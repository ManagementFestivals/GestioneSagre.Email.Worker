{
    "Kestrel": {
        "Limits": {
            "MaxRequestBodySize": 5242880
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "ConnectionStrings": {
        "Default": "Data Source=NOME-SERVER\\NOME-ISTANZA;Initial Catalog=NOME-DATABASE;User ID=USERNAME;Password=PASSWORD",
        "RabbitMq": "amqp://[USERNAME]:[PASSWORD]@[HOSTNAME]:5672"
    },
    "AppSettings": {
        "ApplicationName": "gestionesagre-email",
        "QueuePrefetchCount": 1
    }
}